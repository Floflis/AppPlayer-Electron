{
  "name": "app-player-electron",
  "version": "1.9.3",
  "description": "A minimal Electron application for Construct 2 exported games",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ AppPlayerElectron --platform=win32,linux,darwin --arch=x64,ia32 --out=dist --overwrite --ignore=/tmp --tmpdir=/tmp",
    "build:dev": "electron-packager ./ AppPlayerElectron --out=dist --overwrite --ignore=/tmp --tmpdir=/tmp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Floflis/AppPlayer-Electron"
  },
  "author": "Armaldio and Daniell Mesquita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Floflis/AppPlayer-Electron/issues"
  },
  "homepage": "https://github.com/Floflis/AppPlayer-Electron#readme",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^15.2.0",
    "fs-extra": "^9.1.0",
    "serve-handler": "^6.1.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "11.2.1"
  }
}
